# For more documentation, see: editorconfig.org
root = true

[*]
# Unix-style newlines with a newline ending every file
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.py]
indent_style = space
indent_size = 4

[*.json]
indent_style = space
indent_size = 4
